public class HelloWorld{
    public static void main()
    {
        System.out.println("There are 2 truths and one lie.");
        System.out.println("My duck's name is Joe");
        System.out.println("My duck's name is Bob");
        System.out.println("My duck's name is Steve");

        System.out.println("My duck is the best duck");
        System.out.println("The best duck is my duck");
        
        
        System.out.println("Hi Ian, please stop stalking me.");
        System.out.println("It is kind of creepy.");
        System.out.println("I'll call the police.");
    }
}
